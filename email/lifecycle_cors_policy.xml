<CORSConfiguration>
<CORSRule>
	<ID>Allow sajacosmetics</ID>
    <AllowedOrigin>https://sajacosmetics.com</AllowedOrigin>
    <AllowedOrigin>https://www.sajacosmetics.com</AllowedOrigin>
    <AllowedOrigin>http://www.sajacosmetics.com</AllowedOrigin>
